{
    "name": "dieschmidts/contao-web-server-bundle",
    "type": "contao-bundle",
    "description": "Contao plugin for Symfony WebServerBundle",
    "keywords": ["contao", "plugin", "web-server", "development"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dominik Leonhardt",
            "homepage": "https://www.schmidt24.de"
        }
    ],
    "require": {
        "symfony/web-server-bundle": "^3.3 || ^4.2"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "extra": {
        "contao-manager-plugin": "DieSchmidts\\ContaoWebServerBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "DieSchmidts\\ContaoWebServerBundle\\": "src/"
        }
    }
}
